#include<iostream> 
using namespace std;

void main()
{
	setlocale(LC_ALL, "Rus");     

	cout << "ЛОГИЧЕСКИЕ ОПЕРАЦИИ" << NEW_LINE;
	/*
	операторы сравнения
	< меньше
	> больше
	<= меньше или равно
	>= больше или равно

	операторы равенства
	== равно
	!= неравно

	лог. операторы, инверсия и отриц. инверсия
	&& и
	|| или
	!= не

	*/
	cout << (4 > 5) << NEW_LINE; // False (0)
	cout << (4 < 5) << NEW_LINE; //True (1)	

	int t = 1, r = 2;
	cout << (t == r) << NEW_LINE;// False (0)
	cout << (t != r) << NEW_LINE; //True (1)

	cout << ((r < t) && (5 > 4)) << NEW_LINE;
	cout << ((r < t) || (9 > 11)) << NEW_LINE;
}
