#include<iostream> 
using namespace std;

template <typename T1, typename T2>
T1 Sum1(T1 a, T2 b)
{
	return a + b;
}

void main()
{
	setlocale(LC_ALL, "Rus");     

	cout << "шаблоны функций" << NEW_LINE;
	//это классная вещь
	//вот надо нам сложить еще какие-нибудь переменные, а для этого вновь надо перегружать функцию, а вот оказывается не надо
	//шаблонной функции вообще буква ю с каким типом работать

	cout << Sum1(7.5645, 15) << NEW_LINE; //че могу а
	/*			это пример с typename



	template <typename T1,typename T2>
	T1 Sum1(T1 a, T2 b)
	{
		return a + b;
	}

	это пример с class

	template <typename T1,typename T2>
	T1 Sum1(T1 a, T2 b)
	{
		return a + b;
	}





	если что разницы никакой, так что можно быть спокойным (ТОКО НЕ СМЕШИВАЙ)	
	*/
}
